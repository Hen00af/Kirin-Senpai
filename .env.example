# Discord Bot Token
# Get this from https://discord.com/developers/applications
DISCORD_TOKEN=your_discord_bot_token_here

# AtCoder API URL (optional, defaults to kenkoooo.com)
ATCODER_API_URL=https://kenkoooo.com/atcoder/resources/contests.json

# Update interval for checking contests (optional, default: 10m)
UPDATE_INTERVAL=10m

# Maximum number of contests to display (optional, default: 5)
MAX_CONTESTS=5

# Command prefix (optional, default: !)
COMMAND_PREFIX=!

# Log level (optional, default: INFO)
LOG_LEVEL=INFO